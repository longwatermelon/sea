let str:byte* = galloc(10, byte);

fn main() -> int {
    str[0] = 77;
    str[1] = 79;
    str[2] = 79;

    let m_count:int = 0;
    let o_count:int = 0;

    let idx:int = 0;
    while (idx<3) {
        if (str[idx]==77) {
            m_count = m_count+1;
        } else {
            o_count = o_count+1;
        }
        idx = idx+1;
    }

    syscall(60, m_count, 0, 0);
}
