let str:byte* = galloc(10, byte);

fn main() -> int {
    str[0] = 77;
    str[1] = 79;
    str[2] = 79;

    let result:int = 0;

    if (str[0]==77) {
        result = result + 1;
    }

    if (str[1]==79) {
        result = result + 10;
    }

    if (str[2]==79) {
        result = result + 100;
    }

    syscall(60, result, 0, 0);
}
