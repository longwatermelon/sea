let buffer:int = 0;

fn print_char(c:int) -> void {
    buffer=c;
    syscall(4,1,&buffer,1);
}

fn main() -> int {
    let arr:int* = stalloc(6,int);
    let ind:int = 0;
    while (ind != 3) {
        arr[ind] = 48 + ind;
        ind = ind + 1;
    }
    
    ind = 0;
    while (ind != 3) {
        print_char(arr[ind]);
        ind = ind + 1;
    }

    print_char(10);
}
